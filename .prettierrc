{
  "printWidth": 80,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "none",
  "semi": false,
  "wrap_line_length": 120,
  "wrap_attributes": "auto",
  "proseWrap": "always",
  "arrowParens": "avoid",
  "bracketSpacing": false,
  "jsxBracketSameLine": true,
  "useTabs": false,
  "endOfLine": "lf",
  "overrides": [{
      "files": ".prettierrc",
      "options": {
          "parser": "json"
      }
  }]
}
